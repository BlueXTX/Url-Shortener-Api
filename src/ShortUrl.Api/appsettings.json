{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Postgres": ""
  },
  "LocalDriveFileStorage": {
    "UseRelativePath": true,
    "BasePath": "/images"
  },
  "QrCodeGeneration": {
    "BaseUrl": "https://example.com"
  },
  "Cache": {
    "MaxSize": 512,
    "CacheTokens": true,
    "CacheTokensTime": 60,
    "CacheImages": true,
    "CacheImageTime": 30,
    "SaveImagesToDrive": true
  }
}
